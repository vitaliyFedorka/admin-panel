{
  "name": "admin-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "npx bddgen test && playwright test",
    "test:e2e:ui": "npx bddgen test && playwright test --ui --project=chromium",
    "test:e2e:ci": "npx bddgen test && playwright test --project=chromium --workers=1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "framer-motion": "^11.0.5",
    "next": "^16.0.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.3.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^16.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright-bdd": "^8.4.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
